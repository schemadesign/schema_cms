{
  "name": "schemaUI",
  "version": "0.0.1",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.2.5",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-storyshots": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-jest-diff": "^1.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "inquirer-directory": "^2.2.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.0",
    "jest-watch-typeahead": "^0.2.1",
    "nock": "^10.0.6",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react-app-polyfill": "^0.2.2",
    "require-context.macro": "^1.0.4",
    "sinon-chai": "^3.3.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "yarn run lint:js",
    "lint:js": "eslint .",
    "storybook": "start-storybook --config-dir './.storybook'",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "components/**/*.{js,jsx,ts,tsx}",
      "!components/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/components/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/components/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/components/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/Users/danielcapeletti/schema_cms/frontend/schemaUI/node_modules/jest-watch-typeahead/filename.js",
      "/Users/danielcapeletti/schema_cms/frontend/schemaUI/node_modules/jest-watch-typeahead/testname.js"
    ]
  }
}
