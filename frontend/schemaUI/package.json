{
  "name": "schemaUI",
  "version": "0.0.1",
  "homepage": ".",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "restricted"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "dependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.11.2",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-storyshots": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@svgr/rollup": "^4.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.7.1",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^9.1.2",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-jest-diff": "^1.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-element-attributes": "^2.2.0",
    "husky": "^4.3.0",
    "inquirer-directory": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.0",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^9.2.5",
    "nock": "^10.0.6",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react-app-polyfill": "^0.2.2",
    "require-context.macro": "^1.0.4",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-url": "^2.2.2",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "throttle-debounce": "^2.1.0"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c rollup.config.babel.js",
    "test": "node scripts/test.js",
    "lint": "yarn run lint:js",
    "lint:js": "eslint .",
    "storybook": "start-storybook --config-dir './.storybook'",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "components/**/*.{js,jsx,ts,tsx}",
      "!components/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/components/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/components/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/components/**/?(*.)(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/utils/__tests__/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "./node_modules/jest-watch-typeahead/filename.js",
      "./node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
